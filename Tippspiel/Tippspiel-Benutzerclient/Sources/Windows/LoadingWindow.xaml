<Window x:Class="Tippspiel_Benutzerclient.Sources.Windows.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tippspiel_Benutzerclient.Sources.Windows"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="LoadingWindow" Height="500" Width="1000" AllowsTransparency="True" WindowStyle="None">
    <Window.Background>
        <SolidColorBrush Opacity="0"></SolidColorBrush>
    </Window.Background>
    <Grid>
        <Image gif:ImageBehavior.AnimatedSource="../../Ressources/Images/Logo.gif" gif:ImageBehavior.AutoStart="True"
               gif:ImageBehavior.RepeatBehavior="Forever" gif:ImageBehavior.AnimateInDesignMode="True">
        </Image>
    </Grid>
</Window>
